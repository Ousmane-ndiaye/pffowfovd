                                                <li><a href="javascript:void(0);"><img
                                                            src="{{asset('assets/front/images/demo/user-5.jpg')}}" alt="Starring"></a></li>
                                                <li><a href="javascript:void(0);"><img
                                                            src="{{asset('assets/front/images/demo/user-6.jpg')}}" alt="Starring"></a></li>
                                            </ul>

                                        </div><!-- close .progression-studios-slider-caption-align -->
                                    </div><!-- close .progression-studios-slider-caption-width -->

                                </div><!-- close .container -->

                            </div><!-- close .progression-studios-slider-vertical-align -->
                        </div><!-- close .progression-studios-slider-display-table -->

                        <div class="progression-studios-slider-mobile-background-cover-dark"></div>
                    </div><!-- close .progression-studios-slider-image-background -->
                </li>
            </ul>
        </div><!-- close .progression-studios-slider - See /js/script.js file for options -->
        <!-- Swiper -->
        <div id="liste-secteurs" class="swiper-container dashboard-secteur-pro">
            <div class="swiper-wrapper">
                {% for secteur in secteurs %}
                    <div class="swiper-slide {{ loop.first ? 'active' : '' }}" data-id="{{ secteur.id }}" data-path="{{path('professionnel_secteur_liste')}}">
                        <img src="{{asset('assets/secteurs/' ~ secteur.image )}}" alt="{{ secteur.libelle |raw |e }}">
                        <h6>{{ secteur.libelle |raw |e }}</h6>
                    </div>
                {% endfor %}
            </div>
            <!-- Add Pagination -->
            <div class="swiper-pagination"></div>
            <!-- Add Arrows -->
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
        </div>

        <div class="clearfix"></div>

        <div class="dashboard-container">
            <div id="liste-professionnels" class="row">
                <div class="col-12 col-md-12 col-lg-12 col-xl-12 text-center on-loading-liste">
                    <h5 class="text-muted text-center text-waiting spinner-waiting">Chargement...</h5>
                    <img src="{{asset('assets/front/images/spinner.svg')}}" class="img-fluid text-center spinner-waiting" alt="Patientez svp...">
                </div>
            </div><!-- close .row -->
        </div>
    </main>
{% endblock %}

{% block javascripts %}
    {{parent()}}
    <script src="{{asset('assets/front/js/jquery-asRange.min.js')}}" defer></script><!-- Range Slider JS Plugin -->
    <script src="{{asset('assets/front/js/circle-progress.min.js')}}" defer></script><!-- Circle Progress JS Plugin -->
    <script src="{{asset('assets/front/js/script-dashboard.js')}}" defer></script><!-- Custom Document Ready for Dashboard Only JS -->
    <script src="{{asset('assets/plugins/swiper-4.5.0/dist/js/swiper.min.js')}}" defer></script><!-- Swiper JS Plugin -->

    <script src="{{asset('assets/front/pages/entreprise/dashboard/script.js')}}" defer></script>
{% endblock %}
